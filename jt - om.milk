per_frame_37=
per_frame_38=// Screen Mirroring
per_frame_39=echo_zoom=1; // Default 1 (not the same as regular zoom)
per_frame_40=echo_alpha=.5; // .5 for equally visible (usually what you want) 1 or 0 for all the way on/off
