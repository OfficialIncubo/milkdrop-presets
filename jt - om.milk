per_frame_17=q2=avol_att;
per_frame_18=
per_frame_19=// Color Coordination
per_frame_20=red1=if(above(avol,.25),if(above(avol,1.2),if(above(avol,2),.3 ,.06),1),0);
per_frame_21=green1=if(above(avol,.5),if(above(avol,1),if(above(avol,1.8),0,.2),.8+(.1*cos(time))),0);
per_frame_22=blue1=if(above(avol,.3),if(above(avol,.8),if(above(avol,1.6),.3,.9),.32),1);
per_frame_23=
per_frame_24=// General Variables
per_frame_25=zoom=1; // 1 for no zoom, slightly higher for zoom in, slightly lower for zoom out
per_frame_26=warp=0; // WARP!
