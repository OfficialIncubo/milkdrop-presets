per_frame_13=// Custom Variables
per_frame_14=avol=(bass+mid+treb)/3;
per_frame_15=avol_att=(bass_att+mid_att+treb_att)/3; // attenuated inputs are an average over a short time, so they don't change as quickly as the raw inputs
